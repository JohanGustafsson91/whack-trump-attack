{
  "name": "rock-paper-scissors",
  "version": "1.0.0",
  "description": "Rock Paper Scissors backend",
  "main": "index.js",
  "scripts": {
    "dev": "babel-watch src/server.js",
    "prestart": "babel src --out-dir dist",
    "start": "node dist/server.js"
  },
  "keywords": [],
  "author": "Johan Gustafsson <johan.gustafsson@annevo.se>",
  "license": "ISC",
  "xo": {
    "extends": [],
    "esnext": true,
    "semicolon": false,
    "globals": [
      "document",
      "window"
    ],
    "ignores": [
      "webpack.config.js"
    ],
    "rules": {
      "xo/filename-case": 0,
      "react/jsx-closing-bracket-location": 0,
      "react/jsx-no-bind": 0,
      "no-return-assign": 2,
      "react/jsx-space-before-closing": 0,
      "babel/object-curly-spacing": 0
    }
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "nodemon": "^1.9.2",
    "xo": "^0.17.1"
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-watch": "^2.0.5",
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "envs": "^0.1.6",
    "express": "^4.14.0",
    "express-validator": "^2.20.8",
    "fs": "0.0.2",
    "immutable": "^3.8.1",
    "morgan": "^1.7.0",
    "ramda": "^0.23.0",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-observable": "^0.12.2",
    "redux-socket.io": "^1.3.1",
    "redux-thunk": "^2.1.0",
    "request": "^2.79.0",
    "rxjs": "^5.0.2",
    "shortid": "^2.2.6",
    "socket.io": "^1.7.2"
  }
}
